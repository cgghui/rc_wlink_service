{{- $city := .StoreGet "city" false -}}
{{- $company := .StoreGet .HostCurrent false -}}
{{- if eq $city "" -}}
    {{- $city = index (Split (RandKv "domain_all").val " ") 2 -}}
    {{- .StoreSet "title" $city false -}}
{{- end -}}
{{- if eq $company "" -}}
    {{- $company = RandLib "company" -}}
    {{- .StoreSet .HostCurrent $company false -}}
{{- end -}}
